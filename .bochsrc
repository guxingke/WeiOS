romimage: file=$BXSHARE/BIOS-bochs-latest
cpu: ips=10000000
megs:512
vgaromimage: file=$BXSHARE/VGABIOS-lgpl-latest
vga: extension=none

keyboard: serial_delay=250, paste_delay=100000, keymap=$BXSHARE/keymaps/x11-pc-de.map

ata0: enabled=1, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata1: enabled=1, ioaddr1=0x170, ioaddr2=0x370, irq=15
ata2: enabled=0, ioaddr1=0x1e8, ioaddr2=0x3e0, irq=11
ata3: enabled=0, ioaddr1=0x168, ioaddr2=0x360, irq=9
ata0-master: type=disk, mode=flat, path=$MYHOME/WeiOS/WeiOS, cylinders=100, heads=10, spt=10
ata0-slave: type=disk, mode=flat, path=$MYHOME/WeiOS/fs.img, cylinders=1024, heads=1, spt=1
boot:disk

floppy_bootsig_check: disabled=0

panic: action=ask
error: action=report
info: action=report
debug: action=ignore
debugger_log: -

log: bochsout.txt
mouse: enabled=0
clock: sync=realtime
